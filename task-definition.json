{
    "containerDefinitions": [
        {
            "name": "mlflow-container",
            "image": "$AWS_ACCOUNT_ID.dkr.ecr.$AWS_DEFAULT_REGION.amazonaws.com/$IMAGE_REPO_NAME:$IMAGE_TAG",
            "cpu": 0,
            "memoryReservation": 128,
            "portMappings": [],
            "essential": true,
            "environment": [
                {
                    "name": "AWS_SECRET_KEY_ID",
                    "value": $AWS_SECRET_KEY_ID
                },
                {
                    "name": "DATABASE",
                    "value": "$DATABASE"
                },
                {
                    "name": "PASSWORD",
                    "value": "$PASSWORD"
                },
                {
                    "name": "PORT",
                    "value": "$PORT"
                },
                {
                    "name": "AWS_SECRET_ACCESS_KEY",
                    "value": "$AWS_SECRET_ACCESS_KEY"
                },
                {
                    "name": "BUCKET",
                    "value": "$BUCKET"
                },
                {
                    "name": "HOST",
                    "value": "$HOST"
                },
                {
                    "name": "USERNAME",
                    "value": "$USERNAME"
                },
                {
                    "name": "MLFLOW_TRACKING_USERNAME",
                    "value": "$MLFLOW_TRACKING_USERNAME"
                },
                {
                    "name": "MLFLOW_TRACKING_PASSWORD",
                    "value": "$MLFLOW_TRACKING_PASSWORD"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/mlflow-task",
                    "awslogs-region": "$AWS_DEFAULT_REGION",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "mlflow-task",
    "executionRoleArn": "ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
